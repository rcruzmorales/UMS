@model UMS.Models.UserInfo
 
<div id="pictureLayout">
    
    <h5 id="profileTitle">@Session["FirstName"]'s Profile</h5>
    @if (string.IsNullOrEmpty(Session["FileName"].ToString()))
    {
        <img src="~/Content/Images/user_icon.png" class="img-responsive img-circle" @*width="200"*@ @*height="50"*@/>
    }

    else
    {
        <img src="@Url.Content("~/Content/Images/" + Session["FileName"])" class="img-responsive img-circle" @*width="200"*@ @*height="50"*@/>
    }

    <br/>

    <div class="form-group">
        @Html.Label("Email: ")
        @Html.Raw(Session["Email"])
    </div>
    <div class="form-group">
        @Html.Label("Address: ")
        @Html.Raw(Session["Address"])
    </div>
    <div class="form-group">
        @Html.Label("City: ")
        @Html.Raw(Session["City"])
    </div>
    <div class="form-group">
        @Html.Label("State: ")
        @Html.Raw(Session["State"])
    </div>
    <div class="form-group">
        @Html.Label("Zip Code: ")
        @Html.Raw(Session["Zip"])
    </div>
    <div class="form-group" style="margin-left: 70px;">
        <input type="button" value="Edit" class="btn btn-primary btn-lg EditData" data-toggle="modal" data-id=@Session["Id"] data-target="#editUserModal" />
    </div>
</div>


<div id="AdminEditUserProfile" class="modal fade" role="dialog">
    <div class="modal-dialog" style="margin-top: 7%;">
        <div class="modal-content">
            <div class="modal-header modalHeader">
                <h3 class="modal-title modalTitle text-center">Edit Profile</h3>
                <button type="button" class="close" data-dismiss="modal" id="xClose">&times;</button>
            </div>
            <div class="modal-body">
                <div id="modal-content-Edit"></div>
            </div>
        </div>
    </div>
</div>